
<% provide(:title, (t('Members'))) %>

<% content_for :main do %>
<table>
  <thead>
    <tr>
      <th colspan="3">Member </th>
      <th colspan="3">Description </th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.first_name + member.mid_name + member.last_name %> </td>
        <td><%= member.description %>< /td>
        <td><%= link_to 'Show', member %></td>
        <td><%= link_to 'Edit', edit_member_path(test) %></td>
        <td><%= link_to 'Destroy', member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test', new_member_path %>
<% end %>